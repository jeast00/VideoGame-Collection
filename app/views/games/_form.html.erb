<%= form_for(@game) do |f| %>
    <%= f.text_field :title, placeholder: true %><br><br>

    <%= f.text_field :platform, placeholder: true %><br><br>

    <%= f.label :genre_type %>
    <%= f.collection_select :genre_id, Genre.all, :id, :genre_type %><br><br>

    <%= f.submit "Submit Game" %>

<% end %>